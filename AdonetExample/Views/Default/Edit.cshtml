@model  AdonetExample.Models.EmployeeModel 
@{
    ViewBag.Title = "Edit";
}


<form action="~/Default/Edit" method="post">
    <input type="hidden" name="EmpId" value="@Model.EmpId"/>
    <label>EmpName</label>
    <input type="text" name="EmpName"  value="@Model.EmpName"  class="form-control" /><br />
    <label>EmpSalary</label>
    <input type="text" name="EmpSalary" value="@Model.EmpSalary" class="form-control" /><br />
    <input type="submit" class="btn btn-primary" value="Update" />
</form>

