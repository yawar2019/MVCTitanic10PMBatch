@model List<MVCTitanic10PMBatch.Models.EmployeeModel>
@{
    ViewBag.Title = "SendData4";
}

<h2>SendData4</h2>
<table>
    <tr>
        <th>EmpId</th>
        <th>EmpName</th>
        <th>EmpSalary</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <th>@item.EmpId</th>
            <th>@item.EmpName</th>
            <td>@item.EmpSalary</td>
        </tr>
    }
</table>
